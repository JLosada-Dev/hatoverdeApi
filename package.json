{
  "name": "hatoverde-server",
  "version": "1.0.0",
  "description": "API Sistema de Ganado HatoVerde",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
